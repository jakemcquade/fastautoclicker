{
  "productName": "FastAutoClicker",
  "version": "1.0.0",
  "identifier": ".fastautoclicker",
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "shortDescription": "FastAutoClicker",
    "longDescription": "An open sourced, lightweight, and fast auto clicker designed with a sleek modern interface.",
    "targets": ["deb", "msi", "app", "dmg"],
    "createUpdaterArtifacts": true,
    "category": "Utility",
    "resources": [],
    "active": true,
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "wix": {}
    },
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "active": true,
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEM4MEE0NzNERUQ3OThBRUIKUldUcmlubnRQVWNLeUZPNGMrOE5vb1NpVkhZY0hKc1hxNmYrRUk4VDQyY1AxM2hjTFFWUExNWmgK",
      "endpoints": [
        "https://raw.githubusercontent.com/jakemcquade/fastautoclicker/meta/endpoint.json"
      ],
      "windows": {
        "installMode": "passive"
      }
    }
  },
  "app": {
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    },
    "trayIcon": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "windows": [
      {
        "label": "main",
        "title": "Auto Clicker",
        "width": 440,
        "height": 400,
        "alwaysOnTop": true,
        "resizable": false,
        "fullscreen": false,
        "maximizable": false,
        "decorations": false,
        "transparent": true,
        "shadow": true,
        "url": "index.html"
      }
    ]
  }
}